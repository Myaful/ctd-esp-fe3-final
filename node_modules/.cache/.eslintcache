.jsx",["76"],[],"import React from 'react'\nimport axios from 'axios'\nimport { useEffect } from 'react'\nimport { useParams } from 'react-router-dom'\nimport { useContextGlobal } from '../Components/utils/global.context'\n\n\n\nconst Detail = () => {\n \n  const { state, dispatch }= useContextGlobal();\n  const params = useParams()\n  const urlID = `https://jsonplaceholder.typicode.com/users/${params.id}`\n  \n  useEffect(() => {\n    axios(urlID).then(response => {\n      console.log(response);\n      dispatch ({type: 'ONE_DENTIST', payload: response.data})\n    })\n  })\n\n  return (\n    <div className={state.theme == 'light' ? \"light main\" : \"dark main\"}>\n      <h1>DR: {state.dentistDetail.username}, {state.dentistDetail.name}</h1>\n      <p><img style={{ width: \"200px\" }} src=\"/images/doctor.jpg\" alt=\"doctor\" /></p>\n      <h2> Detalles: </h2>\n      <p>Telefono: {state.dentistDetail.phone}</p>\n      <p>E-mail: {state.dentistDetail.email}</p>\n      <p>Web: {state.dentistDetail.website}</p>\n \n    </div>\n  )\n}\n\nexport default Detail","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Routes\\Home.jsx",["77"],[],"import React from 'react'\nimport Card from '../Components/Card'\nimport { useContextGlobal } from '../Components/utils/global.context'\n\n\n\nconst Home = () => {\n  const{state}=useContextGlobal()\n\n  return (\n    <main className={state.theme == 'light' ? \"light\" : \"dark\"} >\n      <h1>ODONTOLÓGOS</h1>\n      <div className='card-grid'>\n        {state.dentists.map(dentist =>       \n        <Card key={dentist.id} name={dentist.name} username={dentist.username} id={dentist.id}/>)}\n      </div>\n    </main>\n  )\n}\n\nexport default Home","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Routes\\Favs.jsx",["78","79"],[],"import React from \"react\";\nimport Card from \"../Components/Card\";\nimport { useContextGlobal } from '../Components/utils/global.context'\n\n\n//Este componente debera ser estilado como \"dark\" o \"light\" dependiendo del theme del Context\n\nconst Favs = () => {\n  const {state}=useContextGlobal()\n\n  return (\n    <div className={state.theme == 'light' ? \"light main\" : \"dark main\"}>\n      <h1>Odontólogos favoritos</h1>\n      <div className=\"card-grid\">\n        {state.favs.length == 0 ? \n        <h2>No ha seleccionado ningun Odontólogo favorito</h2> :\n        state.favs.map((fav) => \n        <Card key={fav.id} name={fav.name} username={fav.username} id={fav.id}/>)}\n      </div>\n    </div>\n  );\n};\n\nexport default Favs;","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Routes\\Contact.jsx",["80"],[],"import React from 'react'\nimport Form from '../Components/Form'\nimport { useContextGlobal } from '../Components/utils/global.context'\n\n\n\nconst Contact = () => {\n  const {state}=useContextGlobal()\n  return (\n    <div className={state.theme == 'light' ? \"light main\" : \"dark main\"}>\n      <h1 className='title-contac'>¿Quiere saber más?</h1>\n      <p className='title-contac'>Envíanos tus preguntas y nos pondremos en contacto contigo.</p>\n      <Form />\n    </div>\n  )\n}\n\nexport default Contact","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Components\\Card.jsx",["81"],[],"import React from \"react\";\nimport { useContextGlobal } from \"./utils/global.context\";\nimport { useState } from 'react'\nimport { Link } from \"react-router-dom\";\n\n\nconst Card = ({ name, username, id }) => {\n\n  const { state, dispatch } = useContextGlobal();\n\n  \n\n  const getFavs =()=>{\n\n    let favs = state.favs.filter(fav => fav.id == id);\n    let isStored = favs == null || favs.length < 1 ? false : true;\n\n    return isStored;\n  }\n\n  const [dentist, setDentist]= useState (getFavs())\n\n  const addFav = ()=>{\n    // Aqui iria la logica para agregar la Card en el localStorage\n    dispatch ({type: 'ADD_FAV', payload:{name: name, username: username, id: id}})\n    setDentist(true);\n  }\n\n  const removeFav = () => { \n    dispatch({type: 'REMOVE_FAV', payload: {id: id}})\n    setDentist(false);\n  }  \n\n\n  return (\n    <div className=\"card\">\n      <Link to={`/detail/${id}`}>\n        <img style={{ width: \"200px\" }} src=\"./images/doctor.jpg\" alt=\"doctor\" />\n        <h6>Apellido : {name}</h6>\n        <h6>Nombre : {username}</h6>\n      </Link>\n  <button onClick={dentist ? removeFav : addFav} className=\"favButton\" > {dentist ? \"Eliminar de Favorito\" : \"Favorito\"}</button>\n</div>\n  );\n};\n\nexport default Card;","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Components\\Form.jsx",["82"],[],"import React from \"react\";\nimport { useState } from \"react\";\n\n\nconst Form = () => {\n  //Aqui deberan implementar el form completo con sus validaciones\n \n  const emailValidation = /^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/;\n\n  const [name, setName]= useState(\"\");\n  const [mail, setMail]= useState (\"\");\n\n\n  const [Info, setInfo] = useState(false);\n  const [Error, setError] = useState(false);\n\n  \n  const handleSubmit = (e) => {\n\n    e.preventDefault();\n\n    if(name.length > 5 && mail.match(emailValidation)) {\n      setInfo(true);\n      setError(false);\n      console.log(name + mail);\n    } else {\n      setInfo(false);\n      setError(true)\n    }\n  }\n  const onChangeName = (e)=> setName(e.target.value);\n  const onChangeMail = (e)=> setMail(e.target.value);\n\n  return (\n    <div onSubmit={handleSubmit} className=\"form\">\n      <form>\n        <input type=\"text\" placeholder=\"Ingrese su Nombre\"  value={name} onChange= {onChangeName} label=\"Nombre completo\"required=\"required\"></input>\n        <input type=\"mail\" placeholder=\"Ingrese su email\" value={mail} onChange= {onChangeMail}  id= \"mail\" label=\"Email\" required=\"required\"></input>\n        <button type=\"submit\">Enviar</button>\n      </form>\n    \n      {Info && <p>{`Gracias ${name}, te contactaremos cuando antes vía mail`}</p>}\n      {Error && <p style={{color: \"#FF0000\"}}>Por favor, verifica su información nuevamente.</p>}\n    </div>\n  );\n};\n\nexport default Form;\n","C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Routes\\utils\\routes.js",[],[],"C:\\Users\\f16113\\Desktop\\Facu\\Front 3\\Final\\fe3-final-main\\src\\Components\\utils\\global.context.jsx",["83"],[],"import {useContext,createContext,useEffect,useReducer} from \"react\";\nimport axios from \"axios\";\n\n\n\nconst initialState = {theme: \"light\",dentists:[],dentistDetail:{}, favs: JSON.parse(localStorage.getItem('favs')) || []}\n\nconst ContextGlobal = createContext();\n\nconst reducer = (state, action) =>{\n  switch (action.type) {\n    case \"ALL_DENTIST\":\n      return {...state, dentists: action.payload}\n    case \"ONE_DENTIST\":\n      return {...state, dentistDetail: action.payload}\n    case \"HANDLE_THEME\":\n      return { ...state, theme: state.theme === \"light\" ? \"dark\" : \"light\" }\n    case \"ADD_FAV\":\n      return {...state, favs: [...state.favs, action.payload]}\n    case \"REMOVE_FAV\":\n            return {...state, favs: state.favs.filter(fav => fav.id != action.payload.id)\n            }\n    default:\n      throw new Error();\n  }\n}\n\nconst ContextProvider = ({ children }) => {\n  //Aqui deberan implementar la logica propia del Context, utilizando el hook useMemo\n  const [state, dispatch] = useReducer (reducer, initialState)\n  const urlApi= `https://jsonplaceholder.typicode.com/users/`\n\n  useEffect(()=>{\n    axios(urlApi).then(res => {\n      dispatch({type: 'ALL_DENTIST', payload: res.data})})\n  }, [urlApi])\n\n  useEffect(()=>{\n    console.log(state.favs)\n    if(state.favs.length < 1) {\n      localStorage.removeItem('favs')\n    } else {\n    localStorage.setItem('favs', JSON.stringify(state.favs))\n    }\n  },[state.favs])\n\n\n  return (\n    <ContextGlobal.Provider value={{state,dispatch}}>\n      {children}\n    </ContextGlobal.Provider>\n  );\n};\nexport default ContextProvider\nexport const useContextGlobal = ()=> useContext(ContextGlobal)  ",{"ruleId":"84","severity":1,"message":"85","line":5,"column":18,"nodeType":"86","messageId":"87","endLine":5,"endColumn":26},{"ruleId":"88","severity":1,"message":"89","line":8,"column":36,"nodeType":"90","messageId":"91","endLine":8,"endColumn":38},{"ruleId":"88","severity":1,"message":"89","line":10,"column":33,"nodeType":"90","messageId":"91","endLine":10,"endColumn":35},{"ruleId":"88","severity":1,"message":"89","line":20,"column":107,"nodeType":"90","messageId":"91","endLine":20,"endColumn":109},{"ruleId":"88","severity":1,"message":"89","line":23,"column":33,"nodeType":"90","messageId":"91","endLine":23,"endColumn":35},{"ruleId":"88","severity":1,"message":"89","line":11,"column":34,"nodeType":"90","messageId":"91","endLine":11,"endColumn":36},{"ruleId":"88","severity":1,"message":"89","line":12,"column":33,"nodeType":"90","messageId":"91","endLine":12,"endColumn":35},{"ruleId":"88","severity":1,"message":"89","line":15,"column":28,"nodeType":"90","messageId":"91","endLine":15,"endColumn":30},{"ruleId":"88","severity":1,"message":"89","line":10,"column":33,"nodeType":"90","messageId":"91","endLine":10,"endColumn":35},{"ruleId":"88","severity":1,"message":"89","line":15,"column":48,"nodeType":"90","messageId":"91","endLine":15,"endColumn":50},{"ruleId":"92","severity":1,"message":"93","line":8,"column":33,"nodeType":"94","messageId":"95","endLine":8,"endColumn":34,"suggestions":"96"},{"ruleId":"88","severity":1,"message":"97","line":21,"column":69,"nodeType":"90","messageId":"91","endLine":21,"endColumn":71},"no-unused-vars","'dispatch' is assigned a value but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-useless-escape","Unnecessary escape character: \\..","Literal","unnecessaryEscape",["98","99"],"Expected '!==' and instead saw '!='.",{"messa